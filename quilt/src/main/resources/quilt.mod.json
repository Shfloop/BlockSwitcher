{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "${description}",
      "contributors": {
        "${mod_developer}": "Developer"
      },


      "contact": {
        "homepage": "https://example.com/",
        "issues": "https://codeberg.org/CRModders/cosmic-quilt-example/issues",
        "sources": "https://codeberg.org/CRModders/cosmic-quilt-example/"
      },

      "icon": "assets/${mod_id}/icon.png"
    },

    "entrypoints": {
      "init": "com.shfloop.BlockSwitcher.BlockSwitcherQuilt"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": ">=${cosmic_quilt_version}"
      },
      {
        "id": "cosmicreach",
        "versions": ">=${cosmic_reach_version}"
      }
    ]
  },

  "access_widener": "${mod_id}.accesswidener",
  "mixin": [
    "${mod_id}.mixins.json",
    "mixins.${mod_id}.json"
  ]
}
